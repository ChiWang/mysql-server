--exec $INNOCHECKSUM -v $MYSQLD_DATADIR/test/tab1.ibd 2>$MYSQL_TMP_DIR/ver_output

perl;
use strict;
use warnings;
use File::Copy;
my $dir = $ENV{'MYSQL_TMP_DIR'};
opendir(DIR, $dir) or die $!;
my $file= 'ver_output';
# open file in write mode
open IN_FILE,"<", "$dir/$file" or die $!;
open OUT_FILE, ">", "$dir/tmp" or die $!;
while(<IN_FILE>)
{
 #Replace complete file path to filename
 $_=~ s/d:.*trace/d:o,mtrchecksum.trace/g;
  print OUT_FILE $_;
}
close(IN_FILE);
close(OUT_FILE);
#move the new content from tmp file to the orginal file.
move ("$dir/tmp", "$dir/$file");
closedir(DIR);
EOF
