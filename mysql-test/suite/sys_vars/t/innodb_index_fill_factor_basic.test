--source include/have_innodb.inc

#
# show the global and session values;
#
select @@global.innodb_index_fill_factor;
--error ER_INCORRECT_GLOBAL_LOCAL_VAR
select @@session.innodb_index_fill_factor;
show global variables like 'innodb_index_fill_factor';
show session variables like 'innodb_index_fill_factor';
select * from information_schema.global_variables where variable_name='innodb_index_fill_factor';
select * from information_schema.session_variables where variable_name='innodb_index_fill_factor';

#
# test default, min, max value
#
let $innodb_index_fill_factor_orig=`select @@innodb_index_fill_factor`;

set global innodb_index_fill_factor=49;
select @@innodb_index_fill_factor;

set global innodb_index_fill_factor=50;
select @@innodb_index_fill_factor;

set global innodb_index_fill_factor=75;
select @@innodb_index_fill_factor;

set global innodb_index_fill_factor=100;
select @@innodb_index_fill_factor;

set global innodb_index_fill_factor=101;
select @@innodb_index_fill_factor;

eval set global innodb_index_fill_factor=$innodb_index_fill_factor_orig;
